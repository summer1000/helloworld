			<xsd:element minOccurs="0" name="main_birth_date" type="xsd:string"/>
 			<!-- 代表者性別 -->
			<xsd:element minOccurs="0" name="main_sex" type="xsd:string"/>
 			<!-- 代表者性別漢字 -->
			<xsd:element minOccurs="0" name="main_sex_name" type="xsd:string"/>
 			<!-- 代表者住所市区町村CD -->
			<xsd:element minOccurs="0" name="main_municipality_cd" type="xsd:string"/>
 			<!-- 代表者住所都県漢字 -->
			<xsd:element minOccurs="0" name="main_pfts_name" type="xsd:string"/>
 			<!-- 代表者住所都県カナ -->
			<xsd:element minOccurs="0" name="main_pfts_name_kana" type="xsd:string"/>
 			<!-- 代表者住所市区漢字 -->
			<xsd:element minOccurs="0" name="main_city_name" type="xsd:string"/>
 			<!-- 代表者住所市区カナ -->
			<xsd:element minOccurs="0" name="main_city_name_kana" type="xsd:string"/>
 			<!-- 代表者住所以下名カナ -->
			<xsd:element minOccurs="0" name="main_street_address_kana" type="xsd:string"/>
 			<!-- 代表者住所以下名漢字 -->
			<xsd:element minOccurs="0" name="main_street_address" type="xsd:string"/>
 			<!-- 代表者電話番号1 -->
			<xsd:element minOccurs="0" name="main_tel1" type="xsd:string"/>
 			<!-- 代表者電話番号2 -->
			<xsd:element minOccurs="0" name="main_tel2" type="xsd:string"/>
 			<!-- 代表者郵便番号 -->
			<xsd:element minOccurs="0" name="main_zip" type="xsd:string"/>
 			<!-- 備考 -->
			<xsd:element minOccurs="0" name="comment" type="xsd:string"/>
 			<!-- 買取支払先コード -->
			<xsd:element minOccurs="0" name="purchase_payee_cd" type="xsd:string"/>
 			<!-- 買取支払先取引先漢字 -->
			<xsd:element minOccurs="0" name="purchase_payee_acc_name" type="xsd:string"/>
 			<!-- 決裁銀行口座（銀行) -->
			<xsd:element minOccurs="0" name="apv_bank_acc_bank" type="xsd:string"/>
 			<!-- 決裁銀行口座（銀行)本店名 -->
			<xsd:element minOccurs="0" name="apv_bank_acc_bank_name" type="xsd:string"/>
 			<!-- 決裁銀行口座（支店) -->
			<xsd:element minOccurs="0" name="apv_bank_acc_sub_store" type="xsd:string"/>
 			<!-- 決裁銀行口座支店名 -->
			<xsd:element minOccurs="0" name="apv_bank_acc_sub_store_name" type="xsd:string"/>
 			<!-- 決裁銀行口座（種別) -->
			<xsd:element minOccurs="0" name="apv_bank_acc_type" type="xsd:string"/>
 			<!-- 決裁銀行口座（種別)漢字 -->
			<xsd:element minOccurs="0" name="apv_bank_acc_type_name" type="xsd:string"/>
 			<!-- 決裁銀行口座（口座番号) -->
			<xsd:element minOccurs="0" name="apv_bank_acc_cd" type="xsd:string"/>
 			<!-- 決裁銀行口座（口座名義人) -->
			<xsd:element minOccurs="0" name="apv_bank_acc_owner" type="xsd:string"/>
 			<!-- 振込手数料 -->
			<xsd:element minOccurs="0" name="remittance_fee" type="xsd:long"/>
 			<!-- ANSER略番 -->
			<xsd:element minOccurs="0" name="anser_short_cd" type="xsd:string"/>
 			<!-- 管理債権サイン -->
			<xsd:element minOccurs="0" name="admin_sn" type="xsd:string"/>
 			<!-- 管理債権サイン（漢字） -->
			<xsd:element minOccurs="0" name="admin_sn_name" type="xsd:string"/>
 			<!-- 管理債権状態区分 -->
			<xsd:element minOccurs="0" name="admin_status_type" type="xsd:string"/>
 			<!-- 管理債権状態区分（漢字） -->
			<xsd:element minOccurs="0" name="admin_status_type_name" type="xsd:string"/>
 			<!-- 事故発生年月日 -->
			<xsd:element minOccurs="0" name="accident_generate_date" type="xsd:string"/>
 			<!-- 事故終結年月日 -->
			<xsd:element minOccurs="0" name="accident_end_date" type="xsd:string"/>
 			<!-- 事故終結コード -->
			<xsd:element minOccurs="0" name="accident_end_cd" type="xsd:string"/>			
 			<!-- レコードバージョン -->
			<xsd:element minOccurs="0" name="record_version" type="xsd:int"/>
 			<!-- 検索結果区分 -->
			<xsd:element minOccurs="0" name="search_ret_type" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:complexType name="storeSearchReqType">
        <xsd:sequence>
			<!-- 区分 -->
			<xsd:element minOccurs="0" name="flg" type="xsd:string"/>
			
            <xsd:element maxOccurs="1" minOccurs="1" name="storeSearchIn" type="tns:storeSearchInType"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="storeSearchResType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="storeSearchOut" type="tns:storeSearchOutType"/>
        </xsd:sequence>
    </xsd:complexType>

	<xsd:element name="storeSearch" type="tns:storeSearchReqType"/>
	<xsd:element name="storeSearchRes" type="tns:storeSearchResType"/>
</xsd:schema>
